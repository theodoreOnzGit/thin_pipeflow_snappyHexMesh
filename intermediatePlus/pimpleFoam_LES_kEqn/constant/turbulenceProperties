FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  LES;



RAS
{
    RASModel        kOmegaSST;

    turbulence      on;

    printCoeffs     on;
    // alphaK1	    500;
}


LES
{
    LESModel        kEqn;

    turbulence      on;

    printCoeffs     on;
    // alphaK1	    500;
    delta           cubeRootVol;
    kEqnCoeffs
    {
        Ck                  0.094;
        Ce                  1.048;
    }


}


